{
  "name": "@craftnotion/adonisjs-advance-redis-rpc",
  "version": "0.0.3",
  "description": "Advance RPC Provider for Adonisjs, based on Redis pub/sub transport",
  "engines": {
    "node": ">=21"
  },
  "author": "Craftnotion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craftnotion/adonis-advance-redis-rpc.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/craftnotion/adonis-advance-redis-rpc.git"
  },
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests",
    "!build/tests_helpers"
  ],
  "exports": {
    ".": "./build/index.js",
    "./services/main": "./build/services/main.js",
    "./redis_rpc_provider": "./build/providers/redis_rpc_provider.js",
    "./types": "./build/src/types.js"
  },
  "tsup": {
    "entry": [
      "./index.ts",
      "./src/types.ts",
      "./services/main.ts",
      "./providers/redis_rpc_provider.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "prebuild": "npm run lint && npm run clean",
    "build": "npm run compile",
    "postbuild": "npm run copy:templates",
    "release": "npm run  format && npm run  build && npm run  test && npm i && np",
    "version": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "Adonis",
    "Adonis5",
    "Redis",
    "Redis",
    "RPC",
    "RPC"
  ],
  "devDependencies": {
    "@adonisjs/assembler": "^7.0.0",
    "@adonisjs/core": "^6.2.0",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/prettier-config": "^1.2.1",
    "@adonisjs/redis": "^8.0.1",
    "@adonisjs/tsconfig": "^1.2.1",
    "@japa/assert": "^2.1.0",
    "@japa/expect-type": "^2.0.1",
    "@japa/file-system": "^2.2.0",
    "@japa/runner": "^3.1.1",
    "@swc/core": "^1.3.102",
    "@types/node": "^20.10.7",
    "@types/uuid": "^9.0.8",
    "c8": "^9.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.38.0",
    "np": "^9.2.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0",
    "@adonisjs/redis": "^8.0.1"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest",
    "registry": "https://registry.npmjs.org"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "anyBranch": true
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "dependencies": {
    "@poppinss/utils": "^6.7.2",
    "release": "^6.3.1",
    "serialize-error": "^11.0.3",
    "uuid": "^9.0.1"
  }
}